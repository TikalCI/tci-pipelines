@Library ("tci-library") _

import tci.multiJob.phase

pipeline {
    agent { label 'tci-jnlp-node' }
    options {
        timestamps()
        buildDiscarder(logRotator(numToKeepStr: '30'))
    }
    stages {
        stage("test") {
            steps {
                script {
                    def testPhase = new phase(this, true)
                    testPhase.addJob(jobName: "test-tciEnv")
                    testPhase.addJob(jobName: "test-tciEnv")

                    testPhase.run()
                }
            }
        }
    }
}
